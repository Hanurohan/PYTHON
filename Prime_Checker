def prime_checker(n):
    is_Prime = True
    for i in range(2, n):
        if n % i == 0:
            is_Prime = False
            break
    if is_Prime==True:
        print("It's a Prime number")
    else:
        print("It is not a Prime number")

n=int(input("Enter the number to check for prime: "))
prime_checker(n)
